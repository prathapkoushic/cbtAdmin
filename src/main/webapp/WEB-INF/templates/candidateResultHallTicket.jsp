<body>

	<jsp:include page='headerUI.jsp'>
		<jsp:param name="articleId" value="" />
	</jsp:include>
	<div id="page-title">
		<div class="col-sm-3"></div>

		<div class="col-sm-6">

			<div class="panel">
				<div class="panel-body">
					<h3 class="title-hero">
						<b>Candidate Results Report</b>
					</h3>
					<div class="example-box-wrapper">
						<form action="generateCandidateResult" method="post" id="form"
							class="form-horizontal bordered-row" data-parsley-validate=""
							id="demo-form">

							<div class="form-group">
								<label class="control-label col-sm-6" for="select location">Enter
									Hall Ticket No:</label>
								<div class="col-sm-6">
									<input type="number" id="candidate" class="form-control"
										name="candidateId" required="required">

								</div>
							</div>
							<div class="form-group">
								<div class="col-md-6"></div>
								<div class="col-md-6 text-right">
									<button type="submit" class="btn btn-primary" id="sub">Generate
										Report</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
			<!-- <div class="footer">
				<div class="row">
					<div class="col-md-6">
						<div class="footer-info"></div>
					</div>
