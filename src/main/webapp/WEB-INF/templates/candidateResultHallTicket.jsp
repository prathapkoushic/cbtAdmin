		</div>
		<script type="text/javascript">
			$(function() {
				var status = false;
				$('#sub').click(function() {
					status = false;
					var candidate_id = $('#candidate').val();
					if (candidate_id != 0) {
						$.ajax({
							method : 'POST',
							url : 'isHallNoExist',
							cache : false,
							data : {
								candidateId : candidate_id
							},
							ContentType : 'json',
							success : function(data) {
								if (data == 'true') {
									status = true;
									$('#form').submit();
								} else {

									alert('Hall Ticket Nubmer Not Exist!');
									$('#candidate').val('');

								}

							},
							error : function(jqxhr, status, exception) {
								alert(exception, status)

							}

						});
						if (!status) {
							return false;

						} else {
							return true;
						}
					} else {
						alert('Hall Ticket Number Required')
						return false;
					}
				});

			});
		</script>
